{
    "name": "korowai/korowai",
    "description": "Open Source LDAP explorer.",
    "keywords": ["framework", "korowai", "ldap", "browser", "explorer"],
    "license": "MIT",
    "type": "project",
    "require": {
        "php": ">=5.6.4",
        "laravel/lumen-framework": "5.5.*",
        "korowai/framework": "dev-master",
        "vlucas/phpdotenv": "~2.2",
        "dingo/api": "2.0.0-alpha1",
        "nilportugues/hal": "^2.1"
    },
    "require-dev": {
        "fzaninotto/faker": "~1.4",
        "phpunit/phpunit": "~6.0",
        "mockery/mockery": "~0.9",
        "behat/behat": "^3.4"
    },
    "autoload": {
        "psr-4": {
            "App\\": "app/",
            "Korowai\\Framework\\": "src/Korowai/Framework/"
        }
    },
    "autoload-dev": {
        "classmap": [
            "tests/",
            "database/"
        ]
    },
    "scripts": {
        "post-root-package-install": [
            "php -r \"copy('.env.example', '.env');\""
        ]
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "optimize-autoloader": true
}
